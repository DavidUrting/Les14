@model Les14.Domain.Parent

@{
    ViewData["Title"] = "ParentChild";
}

<h2>ParentChild</h2>

<div>
    <h3>Parent @Model.Id heeft deze child records:</h3>
    <table>
        <tr><td>ID</td></tr>
        @foreach (var child in Model.Children)
        {
            <tr><td>@child.Id</td></tr>
        }
    </table>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
